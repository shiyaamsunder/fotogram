{
  "name": "fotogram_frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "autoprefixer": "^9.8.6",
    "framer-motion": "^3.7.0",
    "postcss": "^7.0.35",
    "react": "^17.0.1",
    "react-css-spinners": "^4.0.2",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-topbar-progress-indicator": "^4.1.0",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "npm run build:style && craco start",
    "build": "npm run build:style && craco build",
    "postdeploy": "npm run build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "build:style": "tailwind build src/index.css -o src/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "autoprefixer": "^9.8.6",
    "postcss": "^7.0.35",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
  }
}
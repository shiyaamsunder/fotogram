{
  "name": "fotogram_backend",
  "version": "1.0.0",
  "description": "Backend for FotoGram App which is an Instagram Look-alike",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "server": "nodemon index.js --ignore client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" "
  },
  "author": "Shiyaam Sunder",
  "license": "ISC",
  "dependencies": {
    "cloudinary": "^1.25.1",
    "concurrently": "^6.0.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.15",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "socket.io": "^4.0.1"
  },
  "devDependencies": {}
}
